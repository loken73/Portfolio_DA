  <DataSets>
    <DataSet Name="TicketStatus">
      <Query>
        <DataSourceName>mike</DataSourceName>
        <CommandText>select count(ticket_id) as Total, department as dept1, (CASE WHEN department in ( 'Solid_Waste_Management')
	THEN 'Solid Waste Management'
	WHEN department = 'Public_Works_Traffic_Engineering-10-60'
	THEN 'Public Works: Traffic Engineering'
	WHEN department = 'Citations_and_Tags'
	THEN 'Citation &amp; Tags'
	WHEN department = 'Regulatory_and_Economic_Resources'
	THEN 'Regulatory &amp; Economic Resources'
	WHEN department = 'Enforcement_Section-3-36'
	THEN 'Enforcement Section'
	WHEN department = 'Public_Works_Road_And_Bridges-16-60'
	THEN 'Public Works: Roads &amp; Bridges'
	WHEN department = 'RAAM-27-93'
	THEN 'Parks &amp; Rec: RAAM'
	WHEN department = 'Public_Works_Right_Of_Way-4-60'
	THEN 'Public Works: Right of Way'
	WHEN department = 'Public_Works_traffic_Signals_And_Signs-15-60'
	THEN 'Public Works: Traffic Signals &amp; Signs'
	WHEN department = 'Animal_Services'
	THEN 'Animal Services'
	WHEN department = 'Communications_Department'
	THEN 'Communications'
	WHEN department = 'Public_Works_Construction-6-60'
	THEN 'Public Works: Construction'
	ELSE 'No Department'
	END) as Department FROM Miami311
where ticket_status != ('CLOSED') AND department != 'COM_SW'
GROUP BY (CASE WHEN department in ( 'Solid_Waste_Management')
	THEN 'Solid Waste Management'
	WHEN department = 'Public_Works_Traffic_Engineering-10-60'
	THEN 'Public Works: Traffic Engineering'
	WHEN department = 'Citations_and_Tags'
	THEN 'Citation &amp; Tags'
	WHEN department = 'Regulatory_and_Economic_Resources'
	THEN 'Regulatory &amp; Economic Resources'
	WHEN department = 'Enforcement_Section-3-36'
	THEN 'Enforcement Section'
	WHEN department = 'Public_Works_Road_And_Bridges-16-60'
	THEN 'Public Works: Roads &amp; Bridges'
	WHEN department = 'RAAM-27-93'
	THEN 'Parks &amp; Rec: RAAM'
	WHEN department = 'Public_Works_Right_Of_Way-4-60'
	THEN 'Public Works: Right of Way'
	WHEN department = 'Public_Works_traffic_Signals_And_Signs-15-60'
	THEN 'Public Works: Traffic Signals &amp; Signs'
	WHEN department = 'Animal_Services'
	THEN 'Animal Services'
	WHEN department = 'Communications_Department'
	THEN 'Communications'
	WHEN department = 'Public_Works_Construction-6-60'
	THEN 'Public Works: Construction'
	ELSE 'No Department'
	END), department
HAVING count(ticket_id) &gt; 100
order by Total</CommandText>
      </Query>
      <Fields>
        <Field Name="Total">
          <DataField>Total</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="dept1">
