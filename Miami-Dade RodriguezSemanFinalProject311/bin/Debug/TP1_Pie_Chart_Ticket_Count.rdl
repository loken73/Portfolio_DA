  <DataSets>
    <DataSet Name="TPCount">
      <Query>
        <DataSourceName>Miami</DataSourceName>
        <CommandText>
SELECT (CASE WHEN ticket_priority = ('STANDARD')
	THEN 'Standard'
	WHEN ticket_priority IN ('EMERGNCY')
	THEN 'Emergency'
	ELSE 'All Others' 
	END) as ticket_priority_group, COUNT(ticket_id) AS 'Ticket Count' 
FROM Miami311
GROUP BY (CASE WHEN ticket_priority = ('STANDARD')
	THEN 'Standard'
	WHEN ticket_priority IN ('EMERGNCY')
	THEN 'Emergency'
	ELSE 'All Others' 
	END)</CommandText>
      </Query>
      <Fields>
        <Field Name="ticket_priority_group">
          <DataField>ticket_priority_group</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Ticket_Count">
          <DataField>Ticket Count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
