  <DataSets>
    <DataSet Name="GoalDaysDifference">
      <Query>
        <DataSourceName>Miami</DataSourceName>
        <CommandText>SELECT count(ticket_id) as total, (CASE WHEN department in ('COM_SW', 'Solid_Waste_Management')
	THEN 'Solid Waste'
	WHEN department IN ('Public_Works_Traffic_Engineering-10-60', 'Public_Works_Road_And_Bridges-16-60', 'Public_Works_Right_Of_Way-4-60', 'Public_Works_traffic_Signals_And_Signs-15-60', 'Public_Works_Construction-6-60')
	THEN 'Public Works'
	WHEN department = 'Citations_and_Tags'
	THEN 'Citation &amp; Tags'
	WHEN department = 'Regulatory_and_Economic_Resources'
	THEN 'Economic Resources'
	WHEN department = 'Enforcement_Section-3-36'
	THEN 'Enforcement Section'
	WHEN department = 'RAAM-27-93'
	THEN 'Parks &amp; Rec'
	WHEN department = 'Animal_Services'
	THEN 'Animal Services'
	WHEN department = 'Communications_Department'
	THEN 'Communications'
	ELSE 'No Department'
	END) as Department, issue_type
from Miami311 
group by (CASE WHEN department in ('COM_SW', 'Solid_Waste_Management')
	THEN 'Solid Waste'
	WHEN department IN ('Public_Works_Traffic_Engineering-10-60', 'Public_Works_Road_And_Bridges-16-60', 'Public_Works_Right_Of_Way-4-60', 'Public_Works_traffic_Signals_And_Signs-15-60', 'Public_Works_Construction-6-60')
	THEN 'Public Works'
	WHEN department = 'Citations_and_Tags'
	THEN 'Citation &amp; Tags'
	WHEN department = 'Regulatory_and_Economic_Resources'
	THEN 'Economic Resources'
	WHEN department = 'Enforcement_Section-3-36'
	THEN 'Enforcement Section'
	WHEN department = 'RAAM-27-93'
	THEN 'Parks &amp; Rec'
	WHEN department = 'Animal_Services'
	THEN 'Animal Services'
	WHEN department = 'Communications_Department'
	THEN 'Communications'
	ELSE 'No Department'
	END), issue_type
Having (AVG(goal_days) - AVG(actual_completed_days)) &lt; -30 or (AVG(goal_days) - AVG(actual_completed_days)) &gt; 30 order by total desc</CommandText>
      </Query>
      <Fields>
        <Field Name="total">
          <DataField>total</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="department">
          <DataField>Department</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="issue_type">
          <DataField>issue_type</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
