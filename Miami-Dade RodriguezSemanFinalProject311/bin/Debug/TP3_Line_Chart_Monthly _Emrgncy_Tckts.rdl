  <DataSets>
    <DataSet Name="MonthlyEmergency">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select  DATEPART(MONTH, ticket_created_date) as Month, ticket_priority as 'Ticket Priority', count(ticket_id) as 'Ticket Count', AVG(actual_completed_days) as "Average_Ticket_Time"   FROM Miami311
WHERE ticket_priority ='EMERGNCY'
GROUP BY DATEPART(MONTH, ticket_created_date), ticket_priority;</CommandText>
      </Query>
      <Fields>
        <Field Name="Month">
          <DataField>Month</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Ticket_Priority">
          <DataField>Ticket Priority</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Ticket_Count">
          <DataField>Ticket Count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Average_Ticket_Time">
          <DataField>Average_Ticket_Time</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
