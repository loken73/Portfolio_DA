  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SharedDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Department">
            <Value>=Parameters!Department.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IssueType">
            <Value>=Parameters!IssueType.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DISTINCT Department, issue_type, COUNT(issue_type) AS IssueCount, YEAR(ticket_created_date) AS Year
FROM dbo.Miami311
WHERE Department = @Department AND issue_type = @IssueType
GROUP BY YEAR(ticket_created_date), Department, issue_type</CommandText>
      </Query>
      <Fields>
        <Field Name="Department">
          <DataField>Department</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="issue_type">
          <DataField>issue_type</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="IssueCount">
          <DataField>IssueCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Year">
          <DataField>Year</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DepartParam">
      <Query>
        <DataSourceName>SharedDS</DataSourceName>
        <CommandText>SELECT DISTINCT Department
FROM dbo.Miami311
ORDER BY Department</CommandText>
